<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafxapplication1.NewPassController">
   <children>
      <ImageView fitHeight="720.0" fitWidth="1280.0" layoutX="-14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../pics/password_Plan%20de%20travail%201.jpg" />
         </image>
      </ImageView>
      <PasswordField fx:id="newPass" layoutX="197.0" layoutY="267.0" prefHeight="51.0" prefWidth="397.0" promptText="New Password" />
      <PasswordField fx:id="confirm" layoutX="197.0" layoutY="356.0" prefHeight="51.0" prefWidth="397.0" promptText="Confirm" />
      <Button layoutX="248.0" layoutY="477.0" mnemonicParsing="false" onAction="#passChanged" opacity="0.0" prefHeight="52.0" prefWidth="288.0" style="-fx-background-radius: 100;" text="Confirm" />
      <Button layoutX="859.0" layoutY="368.0" mnemonicParsing="false" onAction="#logout" prefHeight="50.0" prefWidth="288.0" style="-fx-background-radius: 100; -fx-background-color: #10919d;" text="Log out" textFill="WHITE">
         <font>
            <Font size="19.0" />
         </font></Button>
   </children>
</AnchorPane>
