<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane id="AnchorPane" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafxapplication1.AfficherOffreController">
    <children>
      <ImageView fitHeight="720.0" fitWidth="1280.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../pics/admin_Plan%20de%20travail%201.jpg" />
         </image>
      </ImageView>
        <Pane layoutX="425.0" layoutY="235.0" prefHeight="375.0" prefWidth="600.0">
            <children>
                <TableView fx:id="Table" layoutX="4.0" layoutY="14.0" prefHeight="342.0" prefWidth="593.0">
                    <columns>
                        <TableColumn fx:id="id" prefWidth="75.0" text="Id" />
                        <TableColumn fx:id="post" prefWidth="75.0" text="Post" />
                        <TableColumn fx:id="description" prefWidth="114.0" text="description" />
                        <TableColumn fx:id="type" prefWidth="79.0" text="Type" />
                        <TableColumn fx:id="condition" prefWidth="95.0" text="Condition" />
