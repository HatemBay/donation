                    <children>
                        <TextField fx:id="TQuantity_Remaining" promptText="Quantity_Remaining" />
                    </children>
                </HBox>
                <HBox prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <TextField fx:id="TId_admin" promptText="Id_admin" />
                    </children>
                </HBox>
                <HBox prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <TextField fx:id="TId_Association" promptText="Id_Association" />
                    </children>
                </HBox>
            </children>
        </VBox>
        <Button fx:id="home" layoutY="262.0" mnemonicParsing="false" onAction="#toHome" opacity="0.0" prefHeight="56.0" prefWidth="211.0" style="-fx-background-color: #000000;" />
