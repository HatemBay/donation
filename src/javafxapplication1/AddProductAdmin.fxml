            <children>
                <HBox prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <TextField fx:id="TName_Product" promptText="Name_Product" />
                    </children>
                </HBox>
                <HBox prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <TextField fx:id="TQuantity_Total" promptText="Quantity_Total" />
                    </children>
                </HBox>
